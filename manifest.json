{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius/query-anything",
  "description": "Query any API with some selected Text as input and display any output in the browserAction popup",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_area": "navbar",
    "default_popup": "popup.html"
  },
  "icons": {
    "512": "icon.png"
  },
  "manifest_version": 2,
  "name": "Query Anything",
  "options_ui": {
    "open_in_tab": true,
    "page": "options.html"
  },
  "commands": {
    "action_0": {
      "description": "1 query processor"
    },
    "action_1": {
      "description": "2 query processor"
    },
    "action_2": {
      "description": "3 query processor"
    },
    "action_3": {
      "description": "4 query processor"
    },
    "action_4": {
      "description": "5 query processor"
    },
    "action_5": {
      "description": "6 query processor"
    },
    "action_6": {
      "description": "7 query processor"
    },
    "action_7": {
      "description": "8 query processor"
    },
    "action_8": {
      "description": "9 query processor"
    },
    "action_9": {
      "description": "10 query processor"
    }
  },
  "permissions": ["activeTab", "storage"],
  "version": "1.0.1"
}
